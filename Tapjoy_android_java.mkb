#!/usr/bin/env mkb
# Builder mkb file for the java portion of the Tapjoy extension on android
platform JAVA

files
{
    (source/android)
    Tapjoy.java
}

librarypath "$MARMALADE_ROOT/s3e/deploy/plugins/android/android.jar"
librarypath "$MARMALADE_ROOT/s3e/loader/android/s3e_release.jar"
librarypath "$MARMALADE_ROOT/extensions/Tapjoy/tapjoyconnectlibrary.jar"

option output-name=lib/android/Tapjoy.jar
